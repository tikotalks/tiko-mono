{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "sequence",
  "projectType": "application",
  "sourceRoot": "apps/sequence/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist/apps/sequence",
        "skipTypeCheck": true
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "sequence:build",
        "port": 3003
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "sequence:sequence"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "apps/sequence/vitest.config.ts"
      }
    }
  },
  "tags": []
}
