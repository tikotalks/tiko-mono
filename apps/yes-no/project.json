{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "yes-no",
  "projectType": "application",
  "sourceRoot": "apps/yes-no/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist/apps/yes-no",
        "skipTypeCheck": true
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "yes-no:build",
        "port": 3000
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "yes-no:build"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "apps/yes-no/vitest.config.ts"
      }
    }
  },
  "tags": []
}