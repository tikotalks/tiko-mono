# Worker Configuration for Production Performance
# Copy these values to your .env file to enable Cloudflare Worker caching

# Enable the content worker for cached API responses
VITE_USE_CONTENT_WORKER=true

# Production worker URL (using custom domain)
VITE_CONTENT_API_URL=https://content.tikoapi.org

# Version ID for cache busting (update when deploying new content)
VITE_DEPLOYED_VERSION_ID=v1.0.0