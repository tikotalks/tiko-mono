{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "marketing",
  "projectType": "application",
  "sourceRoot": "websites/marketing/src",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist/websites/marketing",
        "skipTypeCheck": true
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "marketing:build",
        "port": 3003
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "marketing:build"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "websites/marketing/vitest.config.ts"
      }
    }
  },
  "tags": []
}
