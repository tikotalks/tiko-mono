{
  "name": "media",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "websites/media/src",
  "projectType": "application",
  "tags": ["scope:website", "type:app"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/websites/media",
        "configFile": "websites/media/vite.config.ts"
      },
      "configurations": {
        "production": {
          "mode": "production"
        }
      },
      "dependsOn": ["^build"]
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "media:build",
        "configFile": "websites/media/vite.config.ts",
        "port": 5174
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "media:build",
        "configFile": "websites/media/vite.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["websites/media/**/*.{ts,tsx,js,jsx,vue}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vue-tsc --noEmit",
        "cwd": "websites/media"
      }
    }
  }
}